extends ../index.html.jade

block title
	title Taylor Palmer | Contact

block css
	link(type="text/css" rel="stylesheet" href="../../css/style.css")

block content
	.layout
		header
			.shade
				.title
					h1 Contact
					h4 The easiest ways to find me
		.container.clearfix
			ul.contact.box-grid-3
				li
					a(href="http://codepen.io/tpalmer75" style="background-color: #777")
						i.fa.fa-envelope
						h4 Email
				li
					a(href="http://twitter.com/thpalmer75" style="background-color: #55ACEE")
						i.fa.fa-twitter
						h4 Twitter
				li
					a(href="http://codepen.io/tpalmer75" style="background-color: #111")
						i.fa.fa-codepen
						h4 Codepen
				li
					a(href="https://www.behance.net/taylorpalmer" style="background-color: #1769ff;")
						i.fa.fa-behance
						h4 Behance
				li
					a(href="http://github.com/tpalmer75" style="background-color: rgb(140, 198, 101);")
						i.fa.fa-github
						h4 Github
				li
					a(href="http://github.com/tpalmer75" style="background-color: #999")
						i.fa.fa-linkedin
						h4 Linked In
				


block scripts
	script(src="../js/jquery-2.1.1.min.js")
	script(src="../js/main.js")